# EditorConfig file for rwcards
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Set default charset for php and templates files
[*.{php,html5}]
charset = utf-8

# 4 space indentation for php files
[*.php]
indent_style = space
indent_size = 4
